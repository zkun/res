{
    "headers": [
        {
            "source": "/*",
            "headers": [
                {
                    "key": "Access-Control-Allow-Origin",
                    "value": "*"
                },
                {
                    "key": "Access-Control-Allow-Methods",
                    "value": "GET, HEAD"
                },
                {
                    "key": "Cache-Control",
                    "value": "max-age=31536000"
                }
            ]
        }
    ]
}